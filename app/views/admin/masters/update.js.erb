<% if @master.errors.present? %>
  <% @master.errors.full_messages.each do |message| %>
    M.toast({html: '<%= message %>', classes: 'red'})
  <% end %>
<% else %>
  $('#master_title_<%= @master.id %>').parents('tr').replaceWith('<%= j render @master %>');
  $(".master-errors_<%= @master.id %>").remove()
<%end%>
